<?xml version="1.0" encoding="utf-8"?>
<!--Description of the XML Configuration file structure. 
<models></models> is the root node.
The root node contains a collection of 1 or more <model></model>
tags that encapsulate the data that describes each model.

Each opening <model id="2-2CFM"> tag has an id attribute that is
set to the short name of the model.

The <name></name> tags enclose the <short></short> and 
<long></long> names of the model.

The <function></function> tags enclose the name of the function
in ModelFunctions.py that contains the algorithm for the model.

The <image></image> tags enclose the name of the image file in the 
images folder that represents schematically the model.

The <inlet_type></inlet_type> tags enclose the inlet type 
(single or dual) of the model.

The <parameters></parameters> tags enclose a collection of 
1 or more <parameter></parameter> tags that enclose the
data that describe each model input parameter.

The <name></name> tags enclose the <short></short> and 
<long></long> names of the parameter.

-->
<models>
  <data_file_path>C:\TRISTAN Data\MR Signal Data</data_file_path>
  <!--Constants used by all the models represented in this XML file.-->
  <constants>
    <constant>
      <!--Repetition time of dynamic SPGR sequence in seconds -->
      <name>TR</name>
      <value>0.00378</value>
    </constant>
    <constant>
      <!-- Temporal resolution in sec -->
      <name>dt</name>
      <value>16</value>
    </constant>
    <constant>
      <!-- Duration of baseline scans -->
      <name>t0</name>
      <value>80</value>
    </constant>
    <constant>
      <!-- Degrees-->
      <name>FA</name>
      <value>15</value>
    </constant>
    <constant>
      <!-- Hz/mM-->
      <name>r1</name>
      <value>5.9</value>
    </constant>
    <constant>
      <!-- Hz-->
      <name>R10a</name>
      <value>0.6667</value>
    </constant>
    <constant>
      <!-- Hz-->
      <name>R10v</name>
      <value>0.6667</value>
    </constant>
    <constant>
      <!-- Hz-->
      <name>R10t</name>
      <value>1.25</value>
    </constant>
  </constants>
  <model id="2-2CFM+2DSPGR">
    <name>
      <short>2-2CFM+2DSPGR</short>
      <long>2 Inlet - Two Compartment Gadoxetate and 2DSPGR Model</long>
    </name>
    <function>DualInletTwoCompartmentGadoxetateAnd2DSPGRModel</function>
    <image>DualInletTwoCompartmentGadoxetateModel.png</image>
    <inlet_type>dual</inlet_type>
    <parameters>
      <parameter>
        <name>
          <short>Fa</short>
          <long>Arterial Flow Fraction</long>
        </name>
        <units>%</units>
        <default>17.0</default>
        <step>1.0</step>
        <precision>2</precision>
        <constraints>
          <lower>0.01</lower>
          <upper>100.00</upper>
        </constraints>
      </parameter>
      <parameter>
        <name>
          <short>Ve</short>
          <long>Extracellular Volume Fraction</long>
        </name>
        <units>%</units>
        <default>20.0</default>
        <step>1.0</step>
        <precision>2</precision>
        <constraints>
          <lower>0.1</lower>
          <upper>100.00</upper>
        </constraints>
      </parameter>
      <parameter>
        <name>
          <short>Fp</short>
          <long>Total Plasma Inflow</long>
        </name>
        <units>mL/min/mL</units>
        <default>1.0</default>
        <step>0.1</step>
        <precision>2</precision>
        <constraints>
          <lower>0.1</lower>
          <upper>100.00</upper>
        </constraints>
      </parameter>
      <parameter>
        <name>
          <short>Kbh</short>
          <long>Biliary Efflux Rate</long>
        </name>
        <units>mL/min/mL</units>
        <default>0.017</default>
        <step>0.01</step>
        <precision>3</precision>
        <constraints>
          <lower>0.01</lower>
          <upper>100.0</upper>
        </constraints>
      </parameter>
      <parameter>
        <name>
          <short>Khe</short>
          <long>Hepatocyte Uptake Rate</long>
        </name>
        <units>mL/min/mL</units>
        <default>0.22</default>
        <step>0.01</step>
        <precision>3</precision>
        <constraints>
          <lower>0.01</lower>
          <upper>100.0</upper>
        </constraints>
      </parameter>
    </parameters>
  </model>
