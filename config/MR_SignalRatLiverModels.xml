<?xml version="1.0" encoding="utf-8"?>
<!--Description of the XML Configuration file structure. 
<models></models> is the root node.
The root node contains a collection of 1 or more <model></model>
tags that encapsulate the data that describes each model.

Each opening <model id="2-2CFM"> tag has an id attribute that is
set to the short name of the model.

The <name></name> tags enclose the <short></short> and 
<long></long> names of the model.

The <function></function> tags enclose the name of the function
in ModelFunctions.py that contains the algorithm for the model.

The <image></image> tags enclose the name of the image file in the 
images folder that represents schematically the model.

The <inlet_type></inlet_type> tags enclose the inlet type 
(single or dual) of the model.

The <parameters></parameters> tags enclose a collection of 
1 or more <parameter></parameter> tags that enclose the
data that describe each model input parameter.

The <name></name> tags enclose the <short></short> and 
<long></long> names of the parameter.

-->
<models>
  <data_file_path>C:\TRISTAN Data\MR Signal Data</data_file_path>
  <!--Plot configuration for all the models represented in this XML file-->
  <plot>
    <y_axis_label>Signal Intensity</y_axis_label>
  </plot>
  <!--Constants used by all the models represented in this XML file.-->
  <constants>
    <constant>
      <!--Repetition time of dynamic SPGR sequence in seconds -->
      <name>TR</name>
      <value>0.013</value>
    </constant>
    <constant>
      <!-- Number of baseline scans -->
      <name>baseline</name>
      <value>5</value>
    </constant>
    <constant>
      <!-- Degrees-->
      <name>FA</name>
      <value>20</value>
    </constant>
    <constant>
      <!-- Hz/mM-->
      <name>r1</name>
      <value>5.5</value>
    </constant>
    <constant>
      <!-- Hz-->
      <name>R10a</name>
      <value>0.74575</value>
    </constant>
    <constant>
      <!-- Hz-->
      <name>R10t</name>
      <value>1.3203</value>
    </constant>
  </constants>
  <model id="HF1-2CFM+2DSPGR">
    <name>
      <short>HF1-2CFM+2DSPGR</short>
      <long>High Flow Single Inlet - Two Compartment Filtration and 2DSPGR Model</long>
    </name>
    <function_path>Models.TwoCompartmentModels.SingleInletModels.HighFlowGadoxetate2DSPGR_Rat.HighFlowGadoxetate2DSPGR_Rat</function_path>
    <function>HighFlowGadoxetate2DSPGR_Rat</function>
    <image>HighFlowSingleInletTwoCompartmentGadoxetateModel.png</image>
    <inlet_type>single</inlet_type>
    <parameters>
      <parameter>
        <name>
          <short>Ve</short>
          <long>Extracellular Volume Fraction</long>
        </name>
        <units>%</units>
        <default>23.0</default>
        <step>1.0</step>
        <precision>2</precision>
        <constraints>
          <lower>0.01</lower>
          <upper>99.99</upper>
        </constraints>
      </parameter>
      <parameter>
        <name>
          <short>Kbh</short>
          <long>Biliary Efflux Rate</long>
        </name>
        <units>mL/min/mL</units>
        <default>0.0918</default>
        <step>0.01</step>
        <precision>4</precision>
        <constraints>
          <lower>0.01</lower>
          <upper>100.0</upper>
        </constraints>
      </parameter>
      <parameter>
        <name>
          <short>Khe</short>
          <long>Hepatocyte Uptake Rate</long>
        </name>
        <units>mL/min/mL</units>
        <default>2.358</default>
        <step>0.1</step>
        <precision>3</precision>
        <constraints>
          <lower>0.0001</lower>
          <upper>100.0</upper>
        </constraints>
      </parameter>
    </parameters>
  </model>
  <model id="HF1-2CFM+3DSPGR">
    <name>
      <short>HF1-2CFM+3DSPGR</short>
      <long>High Flow Single Inlet - Two Compartment Filtration and 3DSPGR Model</long>
    </name>
    <function_path>Models/TwoCompartmentModels/SingleInletModels/HighFlowGadoxetate3DSPGR_Rat</function_path>
    <function>HighFlowGadoxetate3DSPGR_Rat</function>
    <image>HighFlowSingleInletTwoCompartmentGadoxetateModel.png</image>
    <inlet_type>single</inlet_type>
    <parameters>
      <parameter>
        <name>
          <short>Ve</short>
          <long>Extracellular Volume Fraction</long>
        </name>
        <units>%</units>
        <default>23.0</default>
        <step>1.0</step>
        <precision>2</precision>
        <constraints>
          <lower>0.01</lower>
          <upper>99.99</upper>
        </constraints>
      </parameter>
      <parameter>
        <name>
          <short>Kbh</short>
          <long>Biliary Efflux Rate</long>
        </name>
        <units>mL/min/mL</units>
        <default>0.0918</default>
        <step>0.01</step>
        <precision>4</precision>
        <constraints>
          <lower>0.01</lower>
          <upper>100.0</upper>
        </constraints>
      </parameter>
      <parameter>
        <name>
          <short>Khe</short>
          <long>Hepatocyte Uptake Rate</long>
        </name>
        <units>mL/min/mL</units>
        <default>2.358</default>
        <step>0.1</step>
        <precision>3</precision>
        <constraints>
          <lower>0.0001</lower>
          <upper>100.0</upper>
        </constraints>
      </parameter>
    </parameters>
  </model>
</models>




