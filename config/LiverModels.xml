          <short>Ve</short>
          <long>Extracellular Volume Fraction</long>
        </name>
        <units>%</units>
        <default>23.0</default>
        <step>1.0</step>
        <precision>2</precision>
        <constraints>
          <lower>0.0</lower>
          <upper>100.00</upper>
        </constraints>
      </parameter>
      <parameter>
        <name>
          <short>Kbh</short>
